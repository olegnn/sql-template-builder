{
  "presets": [[
      "env", {
        "targets": {
          "node": 7
        }
      }
    ]],
  "plugins": [
    "add-module-exports",
    "transform-es2015-modules-umd",
    "transform-object-rest-spread",
    "babel-plugin-syntax-trailing-function-commas",
    "babel-plugin-transform-es5-property-mutators"
  ]
}
